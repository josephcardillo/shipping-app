<%= form_for @boat do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :location %>
    <%= f.text_field :location %>
    <%= f.label :container_volume %>
    <%= f.text_field :container_volume %>
    <%= f.submit %>
  <% end %>




<!-- form_with assumes ajax request with form -->
